{
  "name": "Click To Copy",
  "description": "Build a list of notes which needs to be copied throughout the whole day!",
  "version": "0.0.4",
  "manifest_version": 3,
  "minimum_chrome_version": "92",
  "action": {
    "default_title": "My List",
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "logo.png"

  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ]
}